<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/back">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="3dp"
        android:elevation="10dp"
        app:cardCornerRadius="8dp"
        app:strokeColor="#8C3F51B5"
        app:strokeWidth="1.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/card_bg">

            <TextView
                android:id="@+id/matchPass_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:fontFamily="@font/roboto_bold"
                android:text="Room Pass : ABCD"
                android:textColor="@color/font"
                android:textSize="10sp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView14"
                app:layout_constraintEnd_toStartOf="@+id/copyRoom_IMG2"
                app:layout_constraintTop_toTopOf="@+id/cardView14" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView14"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginEnd="5dp"
                android:outlineAmbientShadowColor="#33EEEEEE"
                android:outlineSpotShadowColor="#33EEEEEE"
                app:cardBackgroundColor="#33EEEEEE"
                app:cardCornerRadius="50dp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView13"
                app:layout_constraintEnd_toStartOf="@+id/matchPass_TV"
                app:layout_constraintTop_toTopOf="@+id/cardView13">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:background="@drawable/ic_lock"
                        android:backgroundTint="@color/font"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView13"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:outlineAmbientShadowColor="#33EEEEEE"
                android:outlineSpotShadowColor="#33EEEEEE"
                app:cardBackgroundColor="#33EEEEEE"
                app:cardCornerRadius="50dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/matchName_TV"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:background="@drawable/ic_home"
                        android:backgroundTint="@color/font"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/matchName_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/roboto_bold"
                android:text="Match 05 Room Details"
                android:textColor="@color/font"
                android:textSize="10sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="15dp"
                android:background="#4DEEEEEE"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/matchName_TV"
                app:layout_constraintTop_toBottomOf="@+id/matchName_TV"></LinearLayout>

            <TextView
                android:id="@+id/matchID_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:fontFamily="@font/roboto_bold"
                android:text="Room ID : 123456789"
                android:textColor="@color/font"
                android:textSize="10sp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView13"
                app:layout_constraintStart_toEndOf="@+id/cardView13"
                app:layout_constraintTop_toTopOf="@+id/cardView13" />

            <ImageView
                android:id="@+id/copyRoom_IMG"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginStart="5dp"
                android:background="@drawable/ic_copy"
                android:backgroundTint="@color/font"
                app:layout_constraintBottom_toBottomOf="@+id/matchID_TV"
                app:layout_constraintStart_toEndOf="@+id/matchID_TV"
                app:layout_constraintTop_toTopOf="@+id/matchID_TV" />

            <ImageView
                android:id="@+id/copyRoom_IMG2"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginEnd="15dp"
                android:background="@drawable/ic_copy"
                android:backgroundTint="@color/font"
                app:layout_constraintBottom_toBottomOf="@+id/matchPass_TV"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/matchPass_TV" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

</androidx.constraintlayout.widget.ConstraintLayout>